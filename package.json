{
  "name": "ptero-client",
  "version": "1.1.4",
  "description": "type-safe wrapper around the pterodactyl api",
  "author": "defnot001 <https://github.com/defnot001>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/defnot001/ptero-client.git"
  },
  "bugs": {
    "url": "https://github.com/defnot001/ptero-client/issues"
  },
  "homepage": "https://github.com/defnot001/ptero-client#readme",
  "keywords": [
    "api",
    "nodejs",
    "library",
    "wrapper",
    "javascript",
    "typescript",
    "pterodactyl",
    "pterodactyl-api",
    "ptero-client"
  ],
  "scripts": {
    "dev": "ts-node src/test/test.ts",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "postversion": "git push --follow-tags",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "tslib": "^2.4.1",
    "ws": "^8.12.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/ws": "^8.5.4",
    "@vitest/ui": "^0.25.8",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.25.7"
  }
}
